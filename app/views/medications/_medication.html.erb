<div id="<%= dom_id medication %>">
  <p>
    <strong>Name:</strong>
    <%= medication.name %>
  </p>

  <p>
    <strong>Expiration date:</strong>
    <%= medication.expiration_date %>
  </p>

  <p>
    <strong>Dosage:</strong>
    <%= medication.dosage %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= medication.description %>
  </p>

</div>
